<view><text>发送方：</text><input data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]]]]]}}" class="uni-input" value="{{nickname}}" bindinput="__e"/><text>接收方：</text><input data-event-opts="{{[['input',[['__set_model',['','fullname','$event',[]]]]]]}}" class="uni-input" value="{{fullname}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['connect',['$event']]]]]}}" bindtap="__e">连接</button><button data-event-opts="{{[['tap',[['disconnect',['$event']]]]]}}" bindtap="__e">断开</button><button data-event-opts="{{[['tap',[['subscribe',['$event']]]]]}}" bindtap="__e">订阅</button><text>消息：</text><input data-event-opts="{{[['input',[['__set_model',['','msg','$event',[]]]]]]}}" class="uni-input" value="{{msg}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['send',['$event']]]]]}}" bindtap="__e">发送消息</button><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view>{{''+index+":"+item.content+''}}</view></block></view>